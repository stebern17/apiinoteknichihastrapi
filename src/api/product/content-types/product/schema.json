{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Size": {
      "type": "string",
      "required": true
    },
    "Weight": {
      "type": "string",
      "required": true
    },
    "Packaging": {
      "type": "string",
      "required": true
    },
    "Specifiation": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "types": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::type.type",
      "mappedBy": "product"
    }
  }
}
